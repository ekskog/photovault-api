{
  "name": "photovault-api",
  "version": "1.0.0",
  "description": "PhotoVault API - Backend for photo gallery with MinIO storage",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "DEBUG=hbvu:* nodemon server.js",
    "dev:debug:server": "DEBUG=hbvu:server:*,hbvu:api:* nodemon server.js",
    "dev:debug:upload": "DEBUG=hbvu:upload:*,hbvu:image:*,hbvu:storage:* nodemon server.js",
    "dev:debug:auth": "DEBUG=hbvu:auth:* nodemon server.js",
    "dev:debug:db": "DEBUG=hbvu:database:* nodemon server.js",
    "dev:debug:performance": "DEBUG=hbvu:performance:* nodemon server.js",
    "start:debug": "DEBUG=hbvu:* node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "photos",
    "gallery",
    "minio",
    "s3",
    "api"
  ],
  "author": "PhotoVault Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "heic-cli": "^1.1.1",
    "heic-convert": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "multer": "^2.0.1",
    "mysql2": "^3.6.5",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
